        SqsQueueParameters:
          BatchSize: 1
          MaximumBatchingWindowInSeconds: 20
      Tags:
        App: qualys-snapshot-scanner
      Target:
        Ref: SfnCreateSnapshotWrapper
      TargetParameters:
        StepFunctionStateMachineParameters:
          InvocationType: FIRE_AND_FORGET
