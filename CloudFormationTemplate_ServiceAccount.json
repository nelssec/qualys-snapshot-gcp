            await s3Management({ action: "CREATE" });
            const s3BucketNameMaps = {};
            for (const serviceRegion of getServiceRegions()) {
              s3BucketNameMaps[serviceRegion] = getS3BucketName(serviceRegion);
            }
            return s3BucketNameMaps;
