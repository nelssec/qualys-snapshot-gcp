            if (action === "CREATE") {
              await Promise.all(getServiceRegions().map(createS3Region));
              await deleteDanglingS3Buckets(getServiceRegions());
            } else if (action === "DELETE") {
              await Promise.all(getServiceRegions().map(deleteS3Region));
            }
          };
