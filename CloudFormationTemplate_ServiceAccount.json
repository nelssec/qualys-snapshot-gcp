                const accountsResponse = await fetchAccounts(cursor, size);
                for (const account of accountsResponse.content) {
                  const result = processAccount(account, scanParams);
                  mappedTargetAccounts.push(result);
                }
                hasNext = accountsResponse.hasNext || false;
                cursor = accountsResponse.cursor || null;
              } while (hasNext);
              return await this.putCfg({ value: mappedTargetAccounts });
            }
            async findCfg({ lookupValue }) {
              const { Items } = await this.query({
                keyExpression: this.getKeyExpression({
                  configId: this.opts.configId,
                  uid: lookupValue
