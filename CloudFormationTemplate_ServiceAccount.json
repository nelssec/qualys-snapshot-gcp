            const sqsClient = new import_client_sqs2.SQSClient({ region });
            const result = [];
            for (const [groupId, group] of Object.entries(groups)) {
              const task = await createTask({
                groupId,
                group,
                batchId,
                tagConfig,
                tableName,
                additionalTaskInfo,
                taskPrefix
